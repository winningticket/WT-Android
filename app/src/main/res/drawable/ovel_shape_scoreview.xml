<?xml version="1.0" encoding="utf-8"?>
<layer-list
xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape android:shape="oval">

        <padding
            android:top="@dimen/edt_paddingleft"
            android:right="@dimen/aboutus_padding"
            android:left="@dimen/aboutus_padding"
            android:bottom="@dimen/aboutus_padding"/>

        <stroke android:color="@color/colorlightplusgray"
            android:width="1dp"/>
        <corners
           android:radius="@dimen/aboutus_padding"/>

        <size
            android:width="@dimen/public_width"
            android:height="@dimen/public_width"/>

         <solid android:color="@color/colorwhite"/>

    </shape>
</item>

<item android:drawable="@drawable/artboard_blue" />
</layer-list>
