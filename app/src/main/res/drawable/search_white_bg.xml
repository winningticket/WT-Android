<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval">

            <padding
                android:top="@dimen/edt_paddingleft"
                android:right="@dimen/edt_paddingleft"
                android:left="@dimen/edt_paddingleft"
                android:bottom="@dimen/edt_paddingleft"/>

            <corners
                android:radius="@dimen/edt_paddingleft"/>

            <size
                android:width="@dimen/spinner_hight"
                android:height="@dimen/spinner_hight"/>

            <solid android:color="@color/btn_color"/>

        </shape>
    </item>

    <item android:drawable="@drawable/ic_search_white_24dp" />
</layer-list>
