<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IndividulaGameType.Individul_play_course_list">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            android:id="@+id/play_store"
            layout="@layout/custome_profile_toolbarpage" />


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ListView
                android:visibility="gone"
                android:id="@+id/individual_play"
                android:scrollbars="none"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView
                android:visibility="gone"
                android:layout_gravity="center"
                android:minLines="3"
                android:textSize="@dimen/text_size"
                android:textColor="@color/colordarkgray"
                android:id="@+id/player_hitory"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/individual_empty_message" />
        </LinearLayout>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>