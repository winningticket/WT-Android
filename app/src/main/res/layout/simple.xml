<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:padding="@dimen/text_icon_padding"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/edt_paddingleft"
    android:layout_marginEnd="@dimen/edt_paddingleft"
    android:layout_marginBottom="@dimen/aboutus_padding"
    android:layout_marginTop="@dimen/aboutus_padding"
    android:background="@drawable/new_back"
    android:id="@+id/linear_row">
    <TextView
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/text1"
        android:textSize="@dimen/_big_header_text_size"
        android:text="@string/_1"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:textColor="#000000"
        android:textStyle="bold"
        android:paddingTop="@dimen/edt_marg_right" />
    <TextView
        android:layout_gravity="center"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/text2"
        android:text="@string/eagle"
        android:gravity="center"
        android:textSize="@dimen/colum_text_size"
        android:layout_marginTop="-5dp"
        android:textColor="#000000"
        android:paddingBottom="@dimen/edt_marg_right" />
</LinearLayout>