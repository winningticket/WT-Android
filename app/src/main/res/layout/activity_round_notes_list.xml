<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.winningticketproject.RoundNotes.RoundNotesList">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorwhite">

    <LinearLayout
        android:orientation="vertical"
        android:background="@color/colorwhite"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/add_funds"
            layout="@layout/custome_profile_toolbarpage" />


        <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_your_note"
            android:visibility="gone"
            android:layout_margin="@dimen/aboutus_padding"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <TextView
            android:visibility="gone"
            android:id="@+id/tv_note_list"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/no_round_notes_founds"/>

    </LinearLayout>

    </FrameLayout>

</android.support.design.widget.CoordinatorLayout>


